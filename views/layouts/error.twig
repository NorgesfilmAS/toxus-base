{% extends this.viewPath('main') %}

{% block fullContent %}
	<div class="row" style="margin-top: 180px;">
		<div class="col-md-offset-3 col-md-6">
			<h4>{{ message |nl2br }}</h4>
		</div>		
	</div>
	<div class="row">
		<div class="col-md-3"> <i class="glyphicon glyphicon-wrench icon-size-large pull-right"></i></div>
		<div class="col-md-6">
			<p class="statement">No biggie. Chillax. The page you are looking for is not here. Either we moved our site around, or this page never existed in the first place. Please check your url and try again or search our site with the <i class="icon-search"></i> search button above. How about visiting our <a href="sitemap.html">site map?</a></p>
		</div>
	</div>	
	{% if App.params.debug %}
	<div class="row">
		<p> <i class="icon-wrench"></i> 
			code: {{ code }}<br/>
			type: {{ type }}<br />
			file: {{ file }}<br />
			line: {{ line }}<br /><br />
			trace: {{ trace|nl2br }} <br />
		</p>
		<div class="row-fluid">
			<div class="span6 offset3"> </div>
		</div>
		<!-- close row-fluid--> 
	</div>
	{% endif %}
	
	
	
{% endblock %}
	   