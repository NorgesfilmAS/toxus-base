{# render a menu #}
<ul class="navigation">
{% for key, item in menu %}
	<li>	
  <a href="{{ item.url | raw }}" data-placement="bottom" {% if item.tooltip %}title="{{ item.tooltip }}"{% endif %}>
  {% if item.icon is defined %}<i class="{{ item.icon }}"></i>{% endif %}
	{% if item.label %}{{ item.label }}{% endif %}
	</a>
  </li>			
{% endfor %}
</ul>		