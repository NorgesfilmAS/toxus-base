{# 
	Show the message the system in in maintenance mode
 #}
{% extends this.viewPath('message') %}

{% block dialogHeader %}
	<h3 class="panel-title">{{ this.te('Maintenance') }}</h3>
{%endblock %}

	
{% block body %}
	<h3>We are sorry for any inconvenience.</h3>
	<p>We are currently working on the system. During this process you can not use the site. Soon we will be back online.</p>
{% endblock %}
	
{% block dialogFooter %}
{% endblock %}	